{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.4.0"
    }
  },
  "routes": [
    {
      "src": "/favicon.ico",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      },
      "continue": true
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "github": {
    "silent": true
  }
}
